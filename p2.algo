ALGORITHM problem2
VAR
    v1,v2: ARRAY_OF real;
    i,n:INTEGER;
BEGIN
    PROCEDURE calcul (vector1,vector2)
        VAR ps = 0;
    BEGIN 
    FOR i FROM 0 TO vector1.length-1 STEP 1  DO
        ps := vector1[i] * vector2[i] 
    END_FOR

    IF (ps= 0) THEN
    write("orthogonal")
    ELSE
    write("non-orthogonal")


    END_IF
    END
    FOR i FROM 1 TO n STEP 1  DO
    read (vector1);
    read (vector2);

calcul (vector1,vector2);

    END_FOR
END